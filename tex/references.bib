@article{taylorDynamicsSpeciesComposition2001,
  title = {Dynamics in {{Species Composition}} of {{Stream Fish Assemblages}}: {{Environmental Variability}} and {{Nested Subsets}}},
  shorttitle = {Dynamics in {{Species Composition}} of {{Stream Fish Assemblages}}},
  author = {Taylor, Christopher M. and Warren Jr., Melvin L.},
  year = 2001,
