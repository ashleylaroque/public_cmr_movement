@article{fraserExplainingLeptokurticMovement2001,
  title = {Explaining {{Leptokurtic Movement Distributions}}: {{Intrapopulation Variation}} in {{Boldness}} and {{Exploration}}.},
  shorttitle = {Explaining {{Leptokurtic Movement Distributions}}},
  author = {Fraser, Douglas F. and Gilliam, James F. and Daley, Michael J. and Le, An N. and Skalski, Garrick T.},
