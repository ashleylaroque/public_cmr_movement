@article{patrickApplicationMetacommunityTheory2021,
  title = {The Application of Metacommunity Theory to the Management of Riverine Ecosystems},
  author = {Patrick, Christopher J. and Anderson, Kurt E. and Brown, Brown L. and Hawkins, Charles P. and Metcalfe, Anya and Saffarinia, Parsa and Siqueira, Tadeu and Swan, Christopher M. and Tonkin, Jonathan D. and Yuan, Lester L.},
  year = 2021,
