  keywords = {Allee effect,Biota,density-dependent dispersal,density-dependent movement,Ecosystem,interspecific competition,Locomotion,microcosms,Models Biological,Paramecium aurelia,plasticity,Population Dynamics,protists,reaction norm,Tetrahymena},
