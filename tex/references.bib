@article{pineReviewTaggingMethods2003,
  title = {A {{Review}} of {{Tagging Methods}} for {{Estimating Fish Population Size}} and {{Components}} of {{Mortality}}},
  author = {Pine, William E. and Pollock, Kenneth H. and Hightower, Joseph E. and Kwak, Thomas J. and Rice, James A.},
  year = 2003,
  month = oct,
  journal = {Fisheries},
