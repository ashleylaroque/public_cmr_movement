  title = {Retention of {{Passive Integrated Transponder Tags}} in {{Stream-Dwelling Rainbow Trout}}},
  author = {Meyer, Kevin A. and High, Brett and Gastelecutto, Nick and Mamer, Elizabeth R. J. and Elle, F. Steven},
