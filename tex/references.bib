}

@article{merciaiWaterDiversionReduces2018,
  title = {Water Diversion Reduces Abundance and Survival of Two {{Mediterranean}} Cyprinids},
  author = {Merciai, Roberto and Bailey, Larissa L. and Bestgen, Kevin R. and Fausch, Kurt D. and Zamora, Llu{\'i}s and Sabater, Sergi and {Garc{\'i}a-Berthou}, Emili},
  year = 2018,
  month = jan,
  journal = {Ecology of Freshwater Fish},
  volume = {27},
  number = {1},
  pages = {481--491},
  issn = {09066691},
  doi = {10.1111/eff.12363},
  urldate = {2021-09-07},
  abstract = {Water flow is a fundamental characteristic required for the ecological integrity of stream ecosystems. However, populations of many freshwater fishes in the Mediterranean region are threatened by man-\-induced drought due to water diversion. Mark-\-recapture methodology is an effective tool for estimating fish abundance and survival probability, but it has been seldom used with Mediterranean freshwater fish. We tagged over 2,400 individuals of two threatened cyprinids (Mediterranean barbel Barbus meridionalis and Catalan chub Squalius laietanus) inhabiting an Iberian stream affected by water diversion, and used mark-\-recapture methods to evaluate the effects of flow, temperature and depth on their apparent survival and abundance at hydrologically altered and perennial reaches. Based on estimates over the summer drying season, survival of Mediterranean barbel was more than six times higher in an upstream reach with permanent flow than a middle reach impacted by water diversion. Water depth was the most important habitat feature accounting for differences in survival, with both barbel and chub exhibiting higher survival and abundance at permanent sites farther downstream where flow had been restored and in an unaltered tributary. Our results show clear negative impacts of water diversion on fish populations. Both species, but particularly barbel, have life-\-cycle characteristics that may allow populations to recover quickly if natural streamflow was restored by conserving water and reducing water diversion.},
  langid = {english},
  file = {/Users/ashleylaroque/Zotero/storage/R8392PQ7/Merciai et al. - 2018 - Water diversion reduces abundance and survival of .pdf}
}

