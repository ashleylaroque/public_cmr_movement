}

@article{gowanRestrictedMovementResident1994,
  title = {Restricted {{Movement}} in {{Resident Stream Salmonids}}: {{A Paradigm Lost}}?},
  shorttitle = {Restricted {{Movement}} in {{Resident Stream Salmonids}}},
  author = {Gowan, Charles and Young, Michael K. and Fausch, Kurt D. and Riley, Stephen C.},
