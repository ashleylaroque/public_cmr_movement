@article{ersQuantifyingTemporalVariability2014,
  title = {Quantifying Temporal Variability in the Metacommunity Structure of Stream Fishes: The Influence of Non-Native Species and Environmental Drivers},
  shorttitle = {Quantifying Temporal Variability in the Metacommunity Structure of Stream Fishes},
  author = {Ers, T. and S{\'a}ly, P. and Tak{\'a}cs, P. and Higgins, C. L. and B{\'i}r{\'o}, P. and Schmera, D.},
  year = 2014,
  month = jan,
  journal = {Hydrobiologia},
