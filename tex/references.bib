}

@article{kurotaSequentialBayesianMethodology2009,
  title = {A Sequential {{Bayesian}} Methodology to Estimate Movement and Exploitation Rates Using Electronic and Conventional Tag Data: Application to {{Atlantic}} Bluefin Tuna ({{Thunnus}} Thynnus)},
  shorttitle = {A Sequential {{Bayesian}} Methodology to Estimate Movement and Exploitation Rates Using Electronic and Conventional Tag Data},
