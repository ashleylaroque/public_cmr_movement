@article{rlCarbon13EnrichmentBenthic1995,
  title = {Carbon-13 Enrichment in Benthic Compared to Planktonic Algae: Foodweb Implications},
  shorttitle = {Carbon-13 Enrichment in Benthic Compared to Planktonic Algae},
  author = {Rl, France},
