@article{kueflerDensityResourcedependentMovement2013,
  title = {Density- and Resource-Dependent Movement Characteristics in a Rotifer},
  author = {Kuefler, Daniel and Avgar, Tal and Fryxell, John M.},
  year = 2013,
  journal = {Functional Ecology},
  volume = {27},
